{
    "name": "silarhi/cfonb-parser",
    "description": "A PHP Parser for CFONB transactions with zero dependencies",
    "license": "MIT",
    "type": "library",
    "keywords": ["cfonb", "parser", "etebac", "swift"],
    "authors": [
        {
            "name": "SILARHI",
            "email": "hello@silarhi.fr"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.66",
        "phpunit/phpunit": "^10.5.26",
        "phpstan/phpstan": "^2",
        "rector/rector": "^2",
        "vimeo/psalm": "^5.26"
    },
    "autoload": {
        "psr-4": {
            "Silarhi\\Cfonb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silarhi\\Cfonb\\Tests\\": "tests/"
        }
    }
}
