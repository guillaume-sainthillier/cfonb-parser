{
    "name": "silarhi/cfonb-parser",
    "description": "A PHP Parser for CFONB transactions",
    "license": "MIT",
    "type": "library",
    "keywords": ["cfonb", "parser", "etebac", "swift"],
    "authors": [
        {
            "name": "Guillaume Sainthillier",
            "email": "hello@silarhi.fr"
        }
    ],
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.9",
        "infection/infection": "^0.26.16",
        "rector/rector": "^0.14.6",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Silarhi\\Cfonb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silarhi\\Cfonb\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
