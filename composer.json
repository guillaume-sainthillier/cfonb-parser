{
    "name": "silarhi/cfonb-parser",
    "description": "A PHP Parser for CFONB transactions",
    "license": "MIT",
    "type": "library",
    "keywords": ["cfonb", "parser", "etebac", "swift"],
    "authors": [
        {
            "name": "Guillaume Sainthillier",
            "email": "hello@silarhi.fr"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.25",
        "phpunit/phpunit": "^10.3.3",
        "phpstan/phpstan": "^1.10.33",
        "rector/rector": "^0.18",
        "vimeo/psalm": "^5.15"
    },
    "autoload": {
        "psr-4": {
            "Silarhi\\Cfonb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silarhi\\Cfonb\\Tests\\": "tests/"
        }
    }
}
