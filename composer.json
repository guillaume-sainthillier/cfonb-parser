{
    "name": "silarhi/cfonb-parser",
    "description": "A PHP Parser for CFONB transactions",
    "license": "MIT",
    "type": "library",
    "keywords": ["cfonb", "parser", "etebac", "swift"],
    "authors": [
        {
            "name": "Guillaume Sainthillier",
            "email": "hello@silarhi.fr"
        }
    ],
    "require": {
        "php": "^7.2 || ^8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18|^3.0",
        "phpunit/phpunit": "^8.5.21|^9.5.10",
        "phpstan/phpstan": "^1.2",
        "infection/infection": "^0.15.3|^0.25.3",
        "vimeo/psalm": "^4.13"
    },
    "autoload": {
        "psr-4": {
            "Silarhi\\Cfonb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silarhi\\Cfonb\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
